{
  "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "allowJs": true,
      "baseUrl": ".",
      "outDir": "./dist",
      "paths": {
          "@/*": ["./src/"],

      },
      "types": ["./vite-env.d.ts", "vite/client"]
  },

}


